@using Radzen.Blazor.Rendering
<GaugeBand 
    From=@From
    To=@To
    Fill=@Fill
    Stroke=@Stroke
    StrokeWidth=@StrokeWidth
    Radius=@(Scale?.CurrentRadius ?? 0)
    Center=@(Scale?.CurrentCenter ?? new Point())
    StartAngle=@(Scale?.StartAngle ?? 0)
    EndAngle=@(Scale?.EndAngle ?? 0)
    Min=@(Scale?.Min ?? 0)
    Max=@(Scale?.Max ?? 0)
    Size=@Height
/>
